<?xml version="1.0" encoding="UTF-8"?>

<beans>

    <bean name="father" class="com.publicgroup.entity.Father">
        <property name="name" type="java.lang.String" value="老李"></property>
        <ref bean="son"></ref>
        <property name="age" type="java.lang.Integer" value="51"></property>
    </bean>

    <bean name="son" class="com.publicgroup.entity.Son">
        <property name="name" type="java.lang.String" value="小李"></property>
    </bean>
</beans>
