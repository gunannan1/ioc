<?xml version="1.0" encoding="UTF-8"?>

<beans>
    <bean name="son" class="com.icbc.entity.Son">
        <property name="name" type="java.lang.String" value="小王"></property>
    </bean>
    <bean name="father" class="com.icbc.entity.Father">
